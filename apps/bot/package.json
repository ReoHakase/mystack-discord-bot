{
  "name": "bot",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "pnpx @discordx/changelog --src src",
    "dev": "ts-node-esm -r tsconfig-paths/register src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm -r tsconfig-paths/register src/main.ts",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "pnpm vitest",
    "test:coverage": "pnpm vitest run --coverage"
  },
  "dependencies": {
    "@discordx/importer": "1.3.1",
    "@discordx/pagination": "3.5.5",
    "discord.js": "14.16.3",
    "discordx": "11.12.1",
    "dotenv": "16.4.5",
    "znv": "0.4.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.29.3",
    "@vitest/ui": "0.29.3",
    "@types/node": "18.15.3",
    "eslint-config-custom": "workspace:*",
    "nodemon": "2.0.21",
    "ts-node": "10.9.1",
    "tsconfig": "workspace:*",
    "tslib": "2.5.0",
    "typescript": "5.0.2",
    "vitest": "0.29.3"
  }
}
