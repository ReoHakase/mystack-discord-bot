{
  "name": "bot",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "pnpx @discordx/changelog --src src",
    "dev": "ts-node-esm -r tsconfig-paths/register src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm -r tsconfig-paths/register src/main.ts",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "pnpm vitest",
    "test:coverage": "pnpm vitest run --coverage"
  },
  "dependencies": {
    "@discordx/importer": "1.2.1",
    "@discordx/pagination": "3.4.1",
    "discord.js": "14.8.0",
    "discordx": "11.7.1",
    "dotenv": "16.0.3",
    "znv": "0.3.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/ui": "0.34.7",
    "@types/node": "18.19.59",
    "eslint-config-custom": "workspace:*",
    "nodemon": "2.0.22",
    "ts-node": "10.9.2",
    "tsconfig": "workspace:*",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "vitest": "0.34.6"
  }
}
